library(pracma)
# Define a safe detrend function that handles empty or single-element groups
safe_detrend <- function(x) {
if (length(x) > 1) {
return(pracma::detrend(x, tt = "linear"))
} else {
return(rep(NA, length(x)))  # Return NA for groups that can't be detrended
}
}
