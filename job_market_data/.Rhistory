#Create a loop that pulls the controls for each state and creates dataframe to save it
# Function to create synthetic state from controls
create_synthetic_state <- function(state_name, weights_df, filter_data) {
# Extract the weights for the target state
state_weights <- weights_df %>%
filter(State == state_name) %>%
select(Controls)
# Split and extract the control states and their weights
controls_list <- strsplit(as.character(state_weights$Controls), ";")[[1]]
control_states <- do.call(rbind, strsplit(controls_list, " "))
control_states_df <- data.frame(State = control_states[, 1], Weight = as.numeric(control_states[, 2]), stringsAsFactors = FALSE)
# Initialize synthetic value accumulator
synthetic_values <- NULL
# Loop through each control state
for (i in 1:nrow(control_states_df)) {
control_state <- control_states_df$State[i]
weight <- control_states_df$Weight[i]
# Pull the control state's values from the filter_data
control_values <- filter_data %>%
filter(State_Acronym == control_state) %>%
select(year, real_ci_cap)
# Multiply the values by the weight and accumulate them
if (is.null(synthetic_values)) {
synthetic_values <- weight * control_values$real_ci_cap
} else {
synthetic_values <- synthetic_values + (weight * control_values$real_ci_cap)
}
}
# Create a dataframe with the synthetic state values
synthetic_state_df <- data.frame(year = control_values$year, synthetic_real_ci_cap = synthetic_values)
return(synthetic_state_df)
}
