# Run model without interaction in plm
model_plm <- plm(
real_log_nci ~ treated + i(rel_year_did, ref = -1),  # No interaction term
data = stacked_df2,
model = "within",  # Fixed-effects model
effect = "twoways"  # Include both "event_id" and "year_event_id"
)
summary(model_plm)
